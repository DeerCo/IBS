{
  "name": "ibs-backend",
  "version": "1.0.0",
  "description": "A backend server for forming groups, handling marks, managing interviews and distributing files.",
  "main": "app.js",
  "scripts": {
    "start": "PORT=3000 node app",
    "dev": "nodemon app.js --legacy-watch",
    "deploy": "pm2 start --env production",
    "test": "node runTest.js",
    "execute": "NODE_ENV=test PORT=3001 DATABASE_URL=postgres://ibs:password@localhost:5432/testibs PORT=3001 mocha ",
    "migrate": "NODE_ENV=test npx sequelize-cli db:migrate",
    "migrateundo": "NODE_ENV=test npx sequelize-cli db:migrate:undo",
    "seed": "NODE_ENV=test npx sequelize-cli db:seed --seed"
  },
  "repository": {
    "type": "git",
